{
  "type": "commonjs",
  "publisher": "markw65",
  "name": "prettier-extension-monkeyc",
  "displayName": "Prettier-Extension-MonkeyC",
  "description": "A Monkey C extension for the Prettier extension",
  "version": "1.0.8",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "activationEvents": [
    "onDebug",
    "onDebugDynamicConfigurations:omonkeyc",
    "onLanguage:monkeyc",
    "onCommand:workbench.action.tasks.runTask",
    "onCommand:prettiermonkeyc.generateOptimizedProject",
    "onCommand:prettiermonkeyc.buildOptimizedProject",
    "onCommand:prettiermonkeyc.runOptimizedProject",
    "onCommand:prettiermonkeyc.exportOptimizedProject"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Generate Optimized Project",
        "command": "prettiermonkeyc.generateOptimizedProject",
        "category": "Prettier Monkey C"
      },
      {
        "title": "Build Optimized Project",
        "command": "prettiermonkeyc.buildOptimizedProject",
        "category": "Prettier Monkey C"
      },
      {
        "title": "Build and Run Optimized Project",
        "command": "prettiermonkeyc.runOptimizedProject",
        "category": "Prettier Monkey C"
      },
      {
        "title": "Export Optimized Project",
        "command": "prettiermonkeyc.exportOptimizedProject",
        "category": "Prettier Monkey C"
      }
    ],
    "configuration": {
      "title": "Prettier Monkey C",
      "type": "object",
      "properties": {
        "prettierMonkeyC.outputPath": {
          "type": "string",
          "description": "Path to where the optimized project should be generated.",
          "default": "bin/optimized",
          "scope": "window"
        },
        "prettierMonkeyC.ignoredExcludeAnnotations": {
          "type": "string",
          "description": "Semicolon separated list of excludeAnnotations to ignore.",
          "default": "",
          "scope": "window"
        },
        "prettierMonkeyC.ignoredAnnotations": {
          "type": "string",
          "description": "Semicolon separated list of annotations to ignore.",
          "default": "",
          "scope": "window"
        },
        "prettierMonkeyC.ignoredSourcePaths": {
          "type": "string",
          "description": "Regex of source paths to ignore.",
          "default": "",
          "scope": "window"
        }
      }
    },
    "taskDefinitions": [
      {
        "type": "omonkeyc",
        "required": [
          "device"
        ],
        "properties": {
          "device": {
            "type": "string",
            "description": "The device to build for. The special value `export' will do an iq build into bin/export"
          },
          "simulatorBuild": {
            "type": "boolean",
            "description": "Is this build for the simulator."
          },
          "releaseBuild": {
            "type": "boolean",
            "description": "Is this a release build."
          },
          "jungleFiles": {
            "type": "string",
            "description": "If present, overrides the corresponding `monkeyC' setting."
          },
          "developerKeyPath": {
            "type": "string",
            "description": "If present, overrides the corresponding `monkeyC' setting."
          },
          "typeCheckLevel": {
            "type": "string",
            "description": "If present, overrides the corresponding `monkeyC' setting."
          },
          "compilerOptions": {
            "type": "string",
            "description": "If present, overrides the corresponding `monkeyC' setting."
          },
          "compilerWarnings": {
            "type": "string",
            "description": "If present, overrides the corresponding `monkeyC' setting."
          },
          "outputPath": {
            "type": "string",
            "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
          },
          "ignoredExcludeAnnotations": {
            "type": "string",
            "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
          },
          "ignoredAnnotations": {
            "type": "string",
            "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
          },
          "ignoredSourcePaths": {
            "type": "string",
            "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
          }
        }
      }
    ],
    "debuggers": [
      {
        "type": "omonkeyc",
        "label": "Optimized Monkey C",
        "languages": [
          "monkeyc",
          "jungle"
        ],
        "configurationAttributes": {
          "launch": {
            "properties": {
              "stopAtLaunch": {
                "type": "boolean",
                "description": "break immediately when the program launches.",
                "default": false
              },
              "device": {
                "type": "string",
                "description": "Device to run or '${command:GetTargetDevice}' to choose a new device each run."
              },
              "releaseBuild": {
                "type": "boolean",
                "description": "Run a release build."
              },
              "jungleFiles": {
                "type": "string",
                "description": "If present, overrides the corresponding `monkeyC' setting."
              },
              "developerKeyPath": {
                "type": "string",
                "description": "If present, overrides the corresponding `monkeyC' setting."
              },
              "typeCheckLevel": {
                "type": "string",
                "description": "If present, overrides the corresponding `monkeyC' setting."
              },
              "compilerOptions": {
                "type": "string",
                "description": "If present, overrides the corresponding `monkeyC' setting."
              },
              "compilerWarnings": {
                "type": "string",
                "description": "If present, overrides the corresponding `monkeyC' setting."
              },
              "outputPath": {
                "type": "string",
                "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
              },
              "ignoredExcludeAnnotations": {
                "type": "string",
                "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
              },
              "ignoredAnnotations": {
                "type": "string",
                "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
              },
              "ignoredSourcePaths": {
                "type": "string",
                "description": "If present, overrides the corresponding `prettierMonkeyC' setting."
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "omonkeyc",
            "request": "launch",
            "name": "Run Optimized",
            "stopAtLaunch": false,
            "device": "${command:GetTargetDevice}"
          }
        ],
        "configurationSnippets": [
          {
            "label": "Optimized Monkey C: Run Optimized",
            "body": {
              "type": "omonkeyc",
              "request": "launch",
              "name": "Run Optimized",
              "stopAtLaunch": false,
              "device": "^\"\\${command:GetTargetDevice}\""
            }
          }
        ],
        "variables": {
          "GetTargetDevice": "monkeyc.getTargetDevice"
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:uninstall": "node ./uninstall.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.66.0",
    "@vscode/test-electron": "^2.1.2",
    "eslint": "^8.9.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@markw65/monkeyc-optimizer": "file:../monkeyc-optimizer",
    "@markw65/prettier-plugin-monkeyc": "^1.0.3"
  },
  "extensionDependencies": [
    "esbenp.prettier-vscode",
    "garmin.monkey-c"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/markw65/prettier-extension-monkeyc.git"
  },
  "homepage": "https://github.com/markw65/prettier-extension-monkeyc/blob/main/README.md"
}
