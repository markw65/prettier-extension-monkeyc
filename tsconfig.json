{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "commonjs",
    "target": "ES2020",
    "checkJs": true /* Typecheck .js files. */,
    "lib": ["ES2020"],
    "outDir": "build",
    "paths": {
      "@markw65/monkeyc-optimizer/util.js": [
        "node_modules/@markw65/monkeyc-optimizer/build/src/util.d.ts"
      ],
      "@markw65/monkeyc-optimizer/api.js": [
        "node_modules/@markw65/monkeyc-optimizer/build/src/api.d.ts"
      ]
    },
    "sourceMap": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "strict": true,
    "useUnknownInCatchVariables": true
  },
  "include": ["src"],
  "exclude": ["node_modules", "build"]
}
